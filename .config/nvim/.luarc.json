{
  "runtime.version": "LuaJIT",
  "runtime.path": [
    "lua/?.lua",
    "lua/?/init.lua"
  ],
  "diagnostics.globals": [
    "vim",
    "MiniFiles"
  ],
  "workspace.checkThirdParty": false,
  "workspace.library": [
    "$VIMRUNTIME"
  ],
    "diagnostics.disable": [
        "param-type-mismatch",
        "unused-local",
        "unused-function",
        "undefined-global",
        "unknown-diag-code"
    ]
}
